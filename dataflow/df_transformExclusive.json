{
	"name": "df_transformExclusive",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_gen2_parquet",
						"type": "DatasetReference"
					},
					"name": "transformSource"
				},
				{
					"dataset": {
						"referenceName": "ds_gen2_parquet",
						"type": "DatasetReference"
					},
					"name": "nexusSources"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "union1"
				},
				{
					"name": "removeExisting"
				}
			],
			"scriptLines": [
				"source(output(",
				"          userId as string,",
				"          firstname as string,",
				"          lastname as string,",
				"          fullname as string,",
				"          password as string,",
				"          officerTitle as string,",
				"          officerID as string,",
				"          OperatorID as string,",
				"          email as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'sourcefilepath',",
				"     format: 'parquet') ~> transformSource",
				"source(output(",
				"          userId as string,",
				"          firstname as string,",
				"          lastname as string,",
				"          fullname as string,",
				"          password as string,",
				"          officerTitle as string,",
				"          officerID as string,",
				"          OperatorID as string,",
				"          email as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'sourcefilepath',",
				"     format: 'parquet') ~> nexusSources",
				"transformSource, nexusSources union(byName: true)~> union1",
				"union1 aggregate(groupBy(email),",
				"     each(match(name != 'email'), $$ = first($$))) ~> removeExisting"
			]
		}
	}
}